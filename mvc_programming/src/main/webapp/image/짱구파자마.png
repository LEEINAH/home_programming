    function getPcCommentMyAttrFilter(obj, recomAttrGrpId, recomAttrId) {
        if ( settings.UserInfo.isLoginYn != "Y" ) {
            $(obj).prop('checked', false);
            login('', go_CommentTab);
            return;
        }

        if( $(obj).is(':checked') ){
            changeCommentOptionSel(false);
            $('#recomAttrId').val(recomAttrId);
            $('#recomAttrGrpId').val(recomAttrGrpId);
            $('#attr_select_sort').val(recomAttrId).trigger('sync.dropdown');
        }else{
            changeCommentOptionSel(true);
            $('#recomAttrId').val('');
            $('#recomAttrGrpId').val('');
            $('#attr_select_sort').val('00').trigger('sync.dropdown');
        }

        fn_GoCommentPage(1);
    }

    var popupCommentWin;
    function popupReview(url) {
        popupCommentWin = popupWin(url + '&f=itemv', 'popCmt', 770, 860, 'yes', 'no');
    }

    window.addEventListener('message', function(e) {
        if(e.data == 'savePdtEvalComplete') {
            if(popupCommentWin) {
                popupCommentWin.close();
            }
            location.reload();
        }
    });

    $(function(){
        $('.cdtl_ly_write .cdtl_ly_close').on('click', function(e){
            $('.cdtl_ly_write').removeClass('on');
        });
        showPcCommentImgVod();

        // 드롭다운
        ssg.View.review.Event.emitter.on(ssg.View.review.Event.CHANGE_DROPDOWN_OPTION, function(params) {
          if(params.target.id == 'cmt_select_sort'){
            getPcCommentSort(params.value);
          }
          if(params.target.id == 'cmt_select_option_list'){
            getPcCommentOptionSort(params.value);
          }
        })

        // NLP필터
        ssg.View.review.Event.emitter.on(ssg.View.review.Event.CLICK_FILTER, function(params) {
          if(params.isActive == true){
            $('#nlpEntyId').val(params.originalEvent.target.attributes[1].value);
            $('#nlpEntySelected').val('true');
          } else{
            $('#nlpEntySelected').val('false');
          }
          fn_GoCommentPage(1);
        })

        //btn chatGPT 좋아요
        $('.btn_good button').on('click', function () {
            let chk = $(this).siblings('button').hasClass('on');
            if(chk){
                $(this).siblings('button').removeClass('on');
                $(this).addClass('on');
            }else{
                $(this).toggleClass('on');
            }
            if($(this).hasClass('on')){
                var areaunit = $(this).parents("[data-react-unit-type]");
                var unitText = [];
                if($(this).hasClass('good')){
                    unitText.push({
