<script type="text/javascript">
    function fn_login() {
        login();
        return;
    }

    var isFirst = true;
    // ajax 전송 상태
    var sendStatus = "WAIT";

    // 신고내용 영역 최대바이트수 체크
    $('#inputSuggestion').on({
        'keyup paste': function(e) {
            var length = this.value.length;
            var maxLength = 500;

            if(length > maxLength) {
                this.value = this.value.substring(0, maxLength);
                alert(maxLength + '글자까지만 작성이 가능합니다.');
            }

            $("#cnttBytes").html(this.value.length);
        }
    });

    // 등록
    function fn_submit() {
        if(sendStatus != "WAIT"){
            alert("데이터 전송중입니다.");
            return;
        }else{
            sendStatus = "SEND";
        }

        var isValid = validateForm();
        if (!isValid) {
            sendStatus = "WAIT";
            return;
        }
        var formData = $('#submitForm').serialize();

        $.ajax({
            type: "POST",
            url: "/comm/ajaxCommReportSend.ssg",
            dataType: "json",
            data : formData,
            beforeSend: function(xhr, settings){
                $('#submitBtn').attr('disabled', true);
            },
            complete: function(xhr, textStatus){
                $('#submitBtn').attr('disabled', false);
            },
            success: function(responseDto) {
                sendStatus = "WAIT";
                alert(responseDto.resultMsg);
                fn_reportInit();
                $(".cdtl_infoly_opt_close").trigger("click");
            },
            error: function(request, status, error) {
                sendStatus = "WAIT";
                alert("죄송합니다. 신고하기 처리 중 에러가 발생하였습니다.\n다시 시도해 주시기 바랍니다.");
                fn_reportInit();
                $(".cdtl_infoly_opt_close").trigger("click");
            }
        });
    }

    // 초기화
    function fn_reportInit() {
        $('#itemId').val('1000026477087');
        $('#stgChnlCd').val('');
        $('#divcTypeCd').val('10');
        $('#notiMailRcvYn').val('');
        $('#itemSgtAnsDemndYn').val('N');
        $('#itemSgtTypeCd').val('');
        $('#itemStgCntt').val('');
        imgAttachDel();

        // 제품정보 선택 초기화
        $("input:radio[name='briefInfo']").removeAttr("checked");
        $(".custom_rdo").removeClass('on');

        // 상세 설명 초기화
        $('#inputSuggestion').val('');
        $("#cnttBytes").html($("#inputSuggestion").val().length);
        isFirst = true;
    }

    //이미지 첨부 팝업 호출
    function attachImgPopup() {
        popupWin('/comm/popup/popupFileUpload.ssg', '', 500, 310, 'no', 'yes');
    }

    /* 파일첨부관련 - 첨부될 이미지 파일 정보 화면에 노출 */
    function addFileInfo(data) {
        var img =
            '<li>' +
            '<span class="img_area">' +
            '<img src="' + data.url
            + '" alt="" /><button type="button" class="btn_dlt" onclick="imgAttachDel();"><span class="ico_dlt"><span class="blind">이미지 삭제</span></span></button>'
            +
            '</span>' +
            '</li>';
        $('.attach_lst').html(''); //사진추가 버튼 삭제
        $('.attach_lst').append(img).append('<input type="hidden" name="fileNames" value="' + data.fileName + '"><input type="hidden" name="urls" value="' + data.url + '">');
    }

    //이미지 삭제
    function imgAttachDel() {
        $('.attach_lst').html(
            '<li><button type="button" class="btn_upload" onclick="attachImgPopup();return false;"><span class="ico"><span class="blind">파일 업로드</span></span></button></li>' +
            '<li><p class="attach_tx">10MB미만의 <br />jpg,gif,png 파일만 등록 가능 합니다.</p></li>'
        );
    }

    function jsSetText(obj) {
        if(isFirst){
            obj.value = "";
            $("#cnttBytes").html($("#inputSuggestion").val().length);
            isFirst = false;
        }
    }

    // validation 체크
    function validateForm(){
        var briefInfoIdx = $("input:radio[name=briefInfo]:checked").length>0?$("input:radio[name=briefInfo]:checked").val():'';
        if ( briefInfoIdx == '' ) {
            alert("제품정보를 선택하십시오.");
            return false;
        }

        if( isFirst || $('#inputSuggestion').val().trim() == '텍스트를 입력하세요' ){
            if($('#inputSuggestion').val().trim() == ""){
                $('#inputSuggestion').val('');
            }
        }

        if ($('#inputSuggestion').val().length > 500) {
            $('#inputSuggestion').val($('#inputSuggestion').val().substring(0, 500));
            alert('500 글자까지만 작성이 가능합니다.');
            return false;
        }

        $('#itemSgtTypeCd').val($('#briefInfoTypeCd' + briefInfoIdx).val());
        $('#itemStgCntt').val('<' + $('#briefInfoTitle' + briefInfoIdx).val() + ':' + $('#briefInfoMsg' + briefInfoIdx).val() + '> ' + $('#inputSuggestion').val());

        return true;
    }
</script></div>
    
